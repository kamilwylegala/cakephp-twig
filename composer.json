{
    "name": "paulredmond/cakephp-twig",
    "type": "cakephp-plugin",
    "description": "Twig plugin for CakePHP 2",
    "keywords": ["plugin", "cakephp", "view"],
    "homepage": "https://github.com/paulredmond/cakephp-twig",
    "version": "2.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Paul Redmond",
            "email": "paulrredmond@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "twig/twig": "^2.0",
        "symfony/templating": "6.*",
        "symfony/config": "6.*",
        "symfony/finder": "6.*",
        "composer/installers": "^1.3.0"
    },
    "autoload": {
        "psr-0": {
           "TwigPlugin": "Lib/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
